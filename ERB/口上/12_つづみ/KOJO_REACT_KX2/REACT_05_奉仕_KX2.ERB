	;COMがなすがままの場合は汎用リアクションへ
	;=================================================
	;REACT分類が受け入れる
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("受け入れる") && !IS_NOWCOMNAME("なすがまま")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;COMを決めうち。REACT分類条件に該当しても積極的やる/平然とやる以外は汎用リアクションへ
	;=================================================
	;REACT分類が積極的従う/平然と従う
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("積極的従う/平然と従う") && IS_NOWCOMNAME("積極的やる/平然とやる")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;=================================================
	;REACT分類が許しを乞う
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("許しを乞う")
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	;COMを決めうち。REACT分類条件に該当しても恥ずかしがる/消極的やる以外は汎用リアクションへ
	;=================================================
	;REACT分類が消極的従う
	;=================================================
	IF 口上記入チェック(0) && IS_COMGRONAME("消極的従う") && IS_NOWCOMNAME("恥ずかしがる/消極的やる")
		PRINTFORMW 
		RETURN 1
	ENDIF
;条件にヒットしなければ汎用リアクションへ流す
RETURN 0
ENDIF

;派生0、派生1を参考に分岐を追加して使用してください
