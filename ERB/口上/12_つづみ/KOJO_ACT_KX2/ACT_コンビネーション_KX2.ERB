				PRINTFORMW 
			;焦らし無し
			CASEELSE
				PRINTFORMW 
		ENDSELECT
		RETURN 1
	ENDIF
	
	;=================================================
	;その他・条件なし
	;=================================================
	IF 口上記入チェック(0)
		;勃起
		IF TCVAR:MASTER:勃起度 >= 1000
			PRINTFORMW 
		;その他
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF

