;ACTAPPLY2を抜けてきた場合に表示する
@REACTION_MESSAGE
;奉仕系ACTで、REACT分類受け入れる（快感を受け入れる、気持ち良い！、気持ち良くして、もっとハードにして、自慰し始める）の場合
IF MENUMATCH(TFLAG:ACT, "奉仕") && IS_COMGRONAME("受け入れる") && !IS_NOWACTNAME("イラマチオ") && !IS_NOWCOMNAME("なすがまま")
	SELECTCASE GET_COM_RANK()
		CASE "自然な行動"
			PRINTFORML 奉仕を指示された%CALLNAME:MASTER%だが、欲情と快感で頭が真っ白になってしまい
			PRINTFORML 全身がふにゃふにゃでうまく奉仕ができなかった…
		CASE "無理のない行動"
			PRINTFORML %CALLNAME:MASTER%は自分が気持ち良くなることで頭がいっぱいで、
			PRINTFORML あまり上手に%CALLNAME%へ奉仕できなかった…
		CASE "無理をした行動"
			PRINTFORML %CALLNAME:MASTER%は%CALLNAME%への奉仕そっちのけで自分ばかり気持ち良くなってしまい
			PRINTFORML %CALLNAME%に呆れられてしまった…
	ENDSELECT
	PRINTW 
ENDIF

;この箇所を弄る時はCOM側のファイルも開いて確認してるのが現状
;将来的には個別の呼び出しを与えてCOMファイルと統合すべきなのかも知れない
;COMも分類毎にフォルダ分けしてCOM毎にファイルを切り分けるべきか。などと考える今日この頃@/L

SELECTCASE TRAINNAME:SELECTCOM
	CASE "沈黙する"                     ;000,沈黙する
		SELECTCASE GET_COM_RANK()
			CASE "自然な行動"
				IF (IS_NOWACTNAME("アイマスク") && !TFLAG:ACT派生) || TEQUIP:アイマスク
					PRINTFORML 視界を奪われ、%CALLNAME%の様子を窺う事もできない%CALLNAME:MASTER%は、
				ELSE
					PRINTFORML %CALLNAME:MASTER%は%CALLNAME%の様子を窺うような素振りを見せたが、
				ENDIF
				PRINTFORML 何も言えずに黙っていた…
			CASE "無理のない行動"
				PRINTFORML %CALLNAME:MASTER%は何も言わなかった…
			CASE "無理をした行動"
				IF COMCOR_POSI()                            ;実はまんざらでもない
					PRINTFORML %CALLNAME:MASTER%は思わず何か言いかけたが、恥ずかしい内容だったので
					PRINTFORML 言葉を飲み込み、黙っていた…
				ELSE                                        ;実は嫌
					PRINTFORML %CALLNAME:MASTER%は不機嫌そうに押し黙ったままだった…
				ENDIF
		ENDSELECT
	CASE "気弱く応答する"               ;001,気弱く応答する
		IF IS_COMGRONAME("許しを乞う")            ;REACT派生:1:哀願する
			SELECTCASE GET_COM_RANK()
				CASE "自然な行動"
					PRINTFORML %CALLNAME:MASTER%は気弱く小さい声で、機嫌を窺うようにしながら
					PRINTFORML もうこんなことやめてと言った…
				CASE "無理のない行動"
					PRINTFORML %CALLNAME:MASTER%は気弱く、もうこんなことやめてと言った…
				CASE "無理をした行動"
					IF COMCOR_POSI()                        ;実はまんざらでもない
						PRINTFORML %CALLNAME:MASTER%は自分の甘えた気持ちを振り払い、
						PRINTFORML 弱々しいながらも、もうやめてと言った…
					ELSE                                    ;実は嫌
						PRINTFORML %CALLNAME:MASTER%は機嫌を損ねないようになるべく弱々しく、
						PRINTFORML もうこんなことやめてと言った…
					ENDIF
			ENDSELECT
		ELSEIF IS_COMGRONAME("消極的従う")        ;RAECT派生:0:気弱く応答する
			SELECTCASE GET_COM_RANK()
				CASE "自然な行動"
					PRINTFORML %CALLNAME:MASTER%は気弱く小さい声で、機嫌を窺うようにしながら返事した…
				CASE "無理のない行動"
					PRINTFORML %CALLNAME:MASTER%は気弱く返事した…
				CASE "無理をした行動"
					IF COMCOR_POSI()                        ;実はまんざらでもない
						PRINTFORML %CALLNAME:MASTER%は自分の気持ちに薄々気が付きながらも、
						PRINTFORML 気弱に距離を取るような返事をした…
					ELSE                                    ;実は嫌
						PRINTFORML %CALLNAME:MASTER%は機嫌を損ねないようになるべく弱々しく返事した…
					ENDIF
			ENDSELECT
		ENDIF
	CASE "無愛想に応答する"             ;002,無愛想に応答する
		SELECTCASE GET_COM_RANK()
			CASE "自然な行動"
				PRINTFORML %CALLNAME:MASTER%は芯の強い瞳で%CALLNAME%を見つめ、無愛想に応えた…
			CASE "無理のない行動"
				PRINTFORML %CALLNAME:MASTER%は無愛想に\@ IS_NOWACTNAME("押し倒す") ? 応答 # 返事 \@した…
			CASE "無理をした行動"
				IF COMCOR_POSI()                            ;実はまんざらでもない
					PRINTFORML %CALLNAME:MASTER%は思わず何か言いかけたが、恥ずかしい内容だったので
					PRINTFORML 何でもないと無愛想に取り繕った…
				ELSE                                        ;実は嫌
					PRINTFORML %CALLNAME:MASTER%は精一杯の抵抗を込めて無愛想に応えた…
				ENDIF
		ENDSELECT
	CASE "愛想よく応答する"             ;003,愛想よく応答する
		SELECTCASE GET_COM_RANK()
			CASE "自然な行動"
				PRINTFORML %CALLNAME:MASTER%は嬉しそうに%CALLNAME%に微笑みかけ、
				PRINTFORML 愛想よく答えた…
			CASE "無理のない行動"
				PRINTFORML %CALLNAME:MASTER%は愛想よく\@ IS_NOWACTNAME("押し倒す") ? 応答 # 返事 \@した…
			CASE "無理をした行動"
				PRINTFORML %CALLNAME:MASTER%はうまく機嫌を取ろうと作り笑顔で応えた…
		ENDSELECT
	CASE "強気に応答する"               ;004,強気に応答する
		IF IS_NOWACTNAME("押し倒す")              ;REACT派生1：押し倒しに屈しない
			;「無愛想に」「愛想よく」に派生がなく、「強気に」だけ派生フラグがセットしてある理由は、
			;「強気で返事した」と「もうやめろと強く言った」のニュアンスが異なるためです。
			;基準としては、それぞれの地の文に対して同じ口上が表示されるのがよいのか、別の口上を用意したほうがよいのかです。
			;TFLAG:REACT派生 = 1		@COM4で代入されているのでコメントアウト@/L
			PRINTFORML %CALLNAME:MASTER%はもうやめろと強く言った…
		ELSE                                      ;REACT派生0：強気に応答
			SELECTCASE GET_COM_RANK()
