SIF CFLAG:MASTER:トラウマ == 70
	TIMES SOURCE:MASTER:トラウマ , 1.50
;顔面騎乗系解除
CALL FACESITTING_SET(0)
;調教者、調教対象自慰を中断
TEQUIP:自慰中 = 0
;調教者マゾっ気に応じて勃起度上昇
TCVAR:MASTER:今回の勃起 += 50 + (GET_ABL(MASTER, "マゾっ気") - 20) * 4
TFLAG:経過時間 += 4

