
;-----------------------------------------------------------
;ACTABLE一括管理
;-----------------------------------------------------------
@GLOBAL_ACTABLE(ARG)
#FUNCTION
#LOCALSIZE 200
#DIM 参照ターン, 200
#DIM DAYTIME管理, 2

;日時更新
IF DAYTIME管理 != TIME || DAYTIME管理:1 != DAY
	VARSET 参照ターン, -1
	DAYTIME管理 = TIME
	DAYTIME管理:1 = DAY
ENDIF

;ターン内初回
IF 参照ターン:ARG != TFLAG:調教時間
	参照ターン:ARG = TFLAG:調教時間
	IF PERCENT(CFLAG:(1000+ARG))
		LOCAL:ARG = 1
	ELSE
		LOCAL:ARG = 0
	ENDIF
ENDIF

;CFLAG:1000～1199によるACT封印判定
ACTABLE_REASON:ARG = 封印_ACT封印
IF LOCAL:ARG == 1
	;採用不可
	RESULT = 0
	RETURNF 1
ENDIF

;休憩系ACT封印
;SANDBOX限定処理
ACTABLE_REASON:ARG = 封印_SANDBOX休憩封印
IF FLAG:モード == 1 && GROUPMATCH(ARG, 90, 91, 92)
	RESULT = 0
	RETURNF 1
ENDIF

