;☆リアクション後☆
@TRAIN_MESSAGE2_20
SELECTCASE COMGRONAME(TFLAG:REACT分類)
	CASE "受け入れる"
		IF TCVAR:MASTER:イきそう        ;イきそう状態
			IF TCVAR:MASTER:焦らし度 > 1          ;焦らし中
				PRINTFORML %CALLNAME:MASTER%は思わず腰をくねらせて快感の痺れを追いかけるが、
				PRINTFORML %CALLNAME%は面白がるようにローターを揺らしている…
			ELSE                                  ;焦らし終了・焦らし無し
				PRINTFORML %CALLNAME:MASTER%は強い快楽に\@ TALENT:MASTER:オトコ ? 押し殺した呻き声 # 甘く蕩けたような叫び声 \@を上げたが、
				PRINTFORML %CALLNAME%は構わずにローターをぐりぐりと押し付け続けた…
			ENDIF
		ELSE                            ;その他の場合
			SELECTCASE GET_COM_RANK()
				CASE "自然な行動"
					PRINTFORML %CALLNAME:MASTER%は玩具で敏感な突起を嬲られる得体の知れない快感の虜になり
					PRINTFORML だらしない善がり声を上げた…
				CASE "無理のない行動"
					PRINTFORML %CALLNAME:MASTER%は腰を浮かせて無機質な快感に溺れている…
				CASE "無理をした行動"
					PRINTFORML %CALLNAME:MASTER%は奇妙な玩具に嬲られる屈辱に唇を噛むが、
					PRINTFORML 強烈な快感であっという間に表情が蕩けていく…
			ENDSELECT
		ENDIF
		RETURN 2
	CASE "積極的従う", "平然と従う"
		SELECTCASE GET_COM_RANK()
			CASE "自然な行動"
				PRINTFORML %CALLNAME:MASTER%は%CALLNAME%の邪魔にならないように、暴れる腰を自ら抑え付けた…
			CASE "無理のない行動"
				PRINTFORML %CALLNAME:MASTER%は奇妙な玩具で執拗に愛撫されながら
				PRINTFORML %CALLNAME%の言う通りに振る舞い、大人しく脚を広げた…
			CASE "無理をした行動"
				IF COMCOR_POSI()                  ;実は気持ちいい
					PRINTFORML %CALLNAME:MASTER%は%CALLNAME%に玩具で責められながら、
					PRINTFORML 腰が崩れて快感に沈みそうになるのを我慢した…
				ELSE                              ;実は嫌
					PRINTFORML %CALLNAME:MASTER%は股間から湧き上がる快感を必死に意識から追い出しつつ
					PRINTFORML %CALLNAME%の言う通りに振る舞った…
				ENDIF
		ENDSELECT
		RETURN 2
	CASE "許しを乞う"
		SELECTCASE GET_COM_RANK()
			CASE "自然な行動"
				PRINTFORML %CALLNAME:MASTER%は得体の知れない玩具とそれのもたらす感覚に怯え、
				PRINTFORML 助けてと言って身をよじった…
			CASE "無理のない行動"
				PRINTFORML %CALLNAME:MASTER%は玩具が強制的に与える快感に怯えている…
			CASE "無理をした行動"
				IF COMCOR_POSI()                  ;実は気持ちいい
					PRINTFORML %CALLNAME:MASTER%はこんな粗末な玩具などで気持ち良くされてしまうことに怯え、
					PRINTFORML 哀願の視線を%CALLNAME%に向けた…
				ELSE                              ;実は嫌
					PRINTFORML %CALLNAME:MASTER%は%CALLNAME%の手にしたローターから送り込まれる快楽に怯え、
					PRINTFORML 小さくやめてと言った…
				ENDIF
		ENDSELECT
		RETURN 2
	CASE "消極的従う"
		SELECTCASE GET_COM_RANK()
			CASE "自然な行動"
				PRINTFORML %CALLNAME:MASTER%はローターの振動が強制的に与えるくすぐったいような快楽を、
				PRINTFORML 喉と鼻で小さく鳴きながらも、必死に我慢した…
			CASE "無理のない行動"
				PRINTFORML %CALLNAME:MASTER%は顔を染め、強制的に与えられる快感を我慢している…
			CASE "無理をした行動"
				IF COMCOR_POSI()                  ;実は気持ちいい
					PRINTFORML %CALLNAME:MASTER%はこんな玩具でも簡単に感じてしまうことに悔しがり、
					PRINTFORML 歯を食い縛って%CALLNAME%の方を見た…
				ELSE                              ;実は嫌
					PRINTFORML %CALLNAME:MASTER%は%CALLNAME%の手にしたローターから送り込まれる快楽に驚き、
					PRINTFORML 思わず声を上げたが、必死に我慢しようとした…
				ENDIF
		ENDSELECT
		RETURN 2
ENDSELECT

