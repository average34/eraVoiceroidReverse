		ENDSELECT
	ELSE                      ;通常
		IF PANTSCHECK(1, 1)             ;下半身服装が要押し退け
			PRINTFORM %CALLNAME:TARGET%は%TARGET_DESCRIPTION("たまま")%%CALLNAME:MASTER%の
			IF PANTSCHECK(1)                      ;下半身服装が邪魔
				PRINTFORM 服越しに、
			ELSE                                  ;その他
				SIF LOWERJACKET(1) == 1                     ;下半身上着が要押し退け
					PRINTFORM %OUTER_B(MASTER, 0, 1)%の裾から手を入れ、
				SIF LOWERUNDER(1) == 1                      ;下半身下着が要押し退け
					PRINTFORM 下着をずらして、
