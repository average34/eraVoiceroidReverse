	RETURN 0

;最終的に加算する興味値
LTEMP = 0

;暴れるなら常に興味-2
SIF IS_COMGRONAME("暴れる/逃げる/拒否")
	LTEMP -= CFLAG:MASTER:調教レベル < 10 ? 2 # 4

;嫌がるなら常に興味-1 // 暴れる扱いの時は弾くように
SIF IS_NOWCOMNAME("嫌がる") && IS_COMGRONAME("消極的従う")
	LTEMP -= CFLAG:MASTER:調教レベル < 10 ? 1 # 2

