@GAIN_TALENT(ARGS)

;魔力消費量を返す
RESULT:1 = GAIN_TALENT_V(ARGS, 0)
;魔力不足を判定
SIF CFLAG:魔力 < GAIN_TALENT_V(ARGS, 0)
	RETURN 1
;調教レベルを判定
SIF CFLAG:調教レベル < GAIN_TALENT_V(ARGS, 1)
	RETURN 2
RETURN 0

