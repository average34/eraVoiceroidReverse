;────────────────────────────────────
;・Ｃ中心に愛撫する
;・調教対象にペニスがある場合、及びペニスバンド装着時には派生コマンドになる
;・調教対象が自慰中の場合は解除される
;・調教者の手に汚れがある場合不快SOURCEが変動する
;・調教者技巧、愛撫、器用な指先素質に応じて勃起度上昇
;-------------------------------------------------
@KOJO_ACT_KX1_10
;-------------------------------------------------
;・TFLAG:ACT派生(0=愛撫/1=手淫/2=ペニスバンド手淫)
;-------------------------------------------------

;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT10_1, 手淫する
;*************************************************
;・男性器への愛撫を行う、つまり手コキ
;・他の派生と比べてSOURCE:MASTER:快Ｃは+200%
;・調教者技巧、愛撫、器用な指先素質に応じて勃起度上昇
;-------------------------------------------------
;必要に応じて、TEQUIP:オナホールやTCVAR:MASTER:勃起度
;PANTSCHECK(1)等で分岐を設ける事もできます。
;まともにやると手間が尋常じゃないので、効率よく記述する事をお勧めします
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT10_1, 手淫する
;記入チェック（=0, 非表示、1, 表示、2, 表示して媚薬等口上をスキップ）
LOCAL = 0
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL && TFLAG:ACT派生 == 1
	;=================================================
	;初回
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && FIRSTACT(0)
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;調教内初回
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && ONCEACT(0)
		;下半身服装がある場合
		IF PANTSCHECK(1)
			PRINTFORMW 
		;その他
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;調教対象がイキそう
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TCVAR:MASTER:イきそう
		;-------------------------------------------------
		;前から愛撫
		;記入チェック
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && TCVAR:MASTER:位置前後 == 1
			;焦らし度によって分岐
			SELECTCASE TCVAR:MASTER:焦らし度
				;焦らし中
				CASE IS > 1
					PRINTFORMW 
				;焦らし終了
				CASE 1
					PRINTFORMW 
				;焦らし無し
				CASEELSE
					PRINTFORMW 
			ENDSELECT
			RETURN LOCAL:1
		ENDIF
		
		;-------------------------------------------------
		;後ろから愛撫
		;記入チェック
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && TCVAR:MASTER:位置前後 == 2
			;焦らし度によって分岐
			SELECTCASE TCVAR:MASTER:焦らし度
				;焦らし中
				CASE IS > 1
					PRINTFORMW 
				;焦らし終了
				CASE 1
					PRINTFORMW 
				;焦らし無し
				CASEELSE
					PRINTFORMW 
			ENDSELECT
			RETURN LOCAL:1
		ENDIF
		
		;位置前後分岐を埋めるなら必要なし。埋めないならここで補完させましょう
		;不要な分岐は削除、もしくはコメントアウトすることで負荷は軽くなります
		;-------------------------------------------------
		;その他
		;記入チェック
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			;焦らし度によって分岐
			SELECTCASE TCVAR:MASTER:焦らし度
				;焦らし中
				CASE IS > 1
					PRINTFORMW 
				;焦らし終了
				CASE 1
					PRINTFORMW 
				;焦らし無し
				CASEELSE
					PRINTFORMW 
			ENDSELECT
			RETURN LOCAL:1
		ENDIF
	ENDIF
	
	;=================================================
	;連続実行
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT == TFLAG:前ACT
		;-------------------------------------------------
		;顔面騎乗中
		;記入チェック
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && TRUECHECK(TEQUIP:顔面騎乗, TEQUIP:顔面騎乗アナル) > 0
			;前から愛撫
			IF TCVAR:MASTER:位置前後 == 1
				PRINTFORMW 
			;後ろから愛撫
			ELSE
				PRINTFORMW 
			ENDIF
			RETURN LOCAL:1
		ENDIF
		
		;-------------------------------------------------
		;その他
		;記入チェック
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1
			;前から愛撫
			IF TCVAR:MASTER:位置前後 == 1
				PRINTFORMW 
			;後ろから愛撫
			ELSE
				PRINTFORMW 
			ENDIF
			RETURN LOCAL:1
		ENDIF
	ENDIF
	
	;ここを最初に書きましょう
	;=================================================
	;その他・条件なし
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
		;前から愛撫
		IF TCVAR:MASTER:位置前後 == 1
			PRINTFORMW 
		;後ろから愛撫
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
ENDIF
