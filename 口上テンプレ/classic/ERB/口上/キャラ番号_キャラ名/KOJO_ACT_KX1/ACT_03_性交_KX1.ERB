;記入チェック（=0, 非表示、1, 表示、2, 表示して媚薬等口上をスキップ）
LOCAL = 1
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL
	;=================================================
	;調教対象が処女
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TALENT:MASTER:処女
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;調教者が童貞
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TALENT:TARGET:童貞
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;前回中出しの内容は
	;(1=Ｖ性交/2=Ｖ性交奉仕/3=Ｖ性交(空射)/4=Ｖ性交奉仕(空射)/5=Ａ性交/6=Ａ性交奉仕/7=Ａ性交(空射)/8=Ａ性交奉仕(空射))
	;=================================================
	;前回中出し
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:前回中出し && TFLAG:前ターン性交
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;連続(正常位から)
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT派生 == 1
		;調教者がイキそう
		IF TCVAR:TARGET:イきそう
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;正常位、対面座位からは派生しない
	;=================================================
	;体位変更(Ｖ性交から)
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT派生 == 2
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;Ａ性交から
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT派生 == 3
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;基本的に射精するまでは他の行動になる事はないので
	;ペニスバンドによる性交奉仕からの派生、といった按配
	;TFLAG:ACT派生 == 4は0616版時点で、ほぼ無意味な状態と思われ
	;=================================================
	;Ｖ性交奉仕/Ａ性交奉仕から
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GROUPMATCH(TFLAG:ACT派生, 4, 5)
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;実質的にTFLAG:ACT派生 == 0(通常)の箇所
	;=================================================
	;ペニスバンド使用時
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TRUECHECK(!PENIS(TARGET), TEQUIP:調教者ペニスバンド) > 0
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;実質的にTFLAG:ACT派生 == 0(通常)の箇所
	;=================================================
	;その他・条件無し
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
ENDIF

;────────────────────────────────────
;ACT35, アナルセックス
;────────────────────────────────────
;・調教対象の性別に関わらず調教者のふたなり化、ペニスバンド装着によって発生する
;・アナルセックス・調教者が調教対象のＡに挿入する
;・イニシアティブはTARGET(SEX_INITIATIVE(TARGET))
;・調教対象の性別に関わらず調教者のふたなり化、ペニスバンド装着によって発生する
;・TEQUIP(三角木馬/ペニスバンド/バイブ/浣腸器＋プラグ)装着中には発生しない
;・TFLAG:お仕置きフラグが立っていると発生しない
;・後背位、対面座位からは発生しない
;・ターン方針が"ソフト", "異常"だと発生しない
;・調教対象のＡ経験が2未満かつ、技巧2未満だと発生しない
;・TFLAG:ACT派生は前ターンからの状態、及び体位の参照に充てられる
;-------------------------------------------------
@KOJO_ACT_KX1_35
;-------------------------------------------------
;・TFLAG:ACT派生(0=通常/1=連続実行/3=Ｖ性交から/4=Ｖ性交奉仕から/5=Ａ性交奉仕から)
;・アナルセックスの場合TFLAG:ACT派生 == 2は定義されていない
;-------------------------------------------------
;・アナルセックスで童貞を捨てた扱いにはならない
;・TEQUIP(アナルバイブ/アナルビーズ)はACTION_APPLYで解除される
;・REACT分類が暴れるだった場合はキャンセルされる
;・v1.010現在、拒絶した場合のSOURCEリセット処理がない
;-------------------------------------------------
;━━━━━━━━━━━━━━━━━━━━━━━━
;ACT35, アナルセックス
;記入チェック（=0, 非表示、1, 表示、2, 表示して媚薬等口上をスキップ）
LOCAL = 1
;━━━━━━━━━━━━━━━━━━━━━━━━
IF LOCAL
	;前回中出しの内容は
	;(1=Ｖ性交/2=Ｖ性交奉仕/3=Ｖ性交(空射)/4=Ｖ性交奉仕(空射)/5=Ａ性交/6=Ａ性交奉仕/7=Ａ性交(空射)/8=Ａ性交奉仕(空射))
	;=================================================
	;前回中出し
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:前回中出し && TFLAG:前ターン性交
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;連続(アナルセックスから)
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT派生 == 1
		;調教者がイキそう
		IF TCVAR:TARGET:イきそう
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN LOCAL:1
	ENDIF
	
	;=================================================
	;Ｖ性交から
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TFLAG:ACT派生 == 3
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;基本的に射精するまでは他の行動になる事はないので
	;ペニスバンドによる性交奉仕からの派生、といった按配
	;TFLAG:ACT派生 == 4は0616版時点で、ほぼ無意味な状態と思われ
	;=================================================
	;Ｖ性交奉仕/Ａ性交奉仕から
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && GROUPMATCH(TFLAG:ACT派生, 4, 5)
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;実質的にTFLAG:ACT派生 == 0(通常)の箇所
	;=================================================
	;ペニスバンド使用時
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1 && TRUECHECK(!PENIS(TARGET), TEQUIP:調教者ペニスバンド) > 0
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
	
	;実質的にTFLAG:ACT派生 == 0(通常)の箇所
	;地の文はABL:MASTER:Ａ感覚 > 3で開発済み、ABL:MASTER:Ａ感覚 > 1で開発途中
	;その他は未開発という分岐になっている。
	;発生条件に一定以上のＡ感覚が必要なので、あまり考慮する必要はないかも知れない
	;=================================================
	;その他・条件無し
	;記入チェック
	LOCAL:1 = 0
	;=================================================
	IF LOCAL:1
		PRINTFORMW 
		RETURN LOCAL:1
	ENDIF
ENDIF